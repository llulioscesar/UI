    "@types/styled-components": "4.1.8",
    "copyfiles": "^2.1.1",
    "css-loader": "^6.7.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "react": "^16.10.2",
    "style-loader": "^1.3.0",
